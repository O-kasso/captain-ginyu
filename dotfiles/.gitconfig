[filter "media"]
	clean = git media clean %f
	smudge = git media smudge %f
	required = true
[user]
	name = Omar Kassouar
	email = omar@kassouar.com
[push]
	default = simple
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
[core]
	excludesfile = "$HOME"/.gitignore
[credential]
	helper = osxkeychain
[pull]
	rebase = true
